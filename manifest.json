{
    "name": "YExtension",
    "description": "Chrome Extension by eternity6666",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ],
    "action": {
        "default_popup": "main.html",
        "default_icon": {
            "16": "./img/16.png",
            "48": "./img/48.png",
            "128": "./img/128.png"
        }
    },
    "icons": {
        "16": "./img/16.png",
        "48": "./img/48.png",
        "128": "./img/128.png"
    }
}